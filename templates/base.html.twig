<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">

        <title>{% block title %}Welcome!{% endblock %}</title>

      
    

      

        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
       
      <!-- Latest compiled and minified CSS -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
          <link rel="stylesheet" href="{{asset('css/style.css')}}">
       

        {% block stylesheets %}
        {% endblock %}

        {% block javascripts %}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-lg bg-secondary justify-content-center">
            <div class="container-fluid">
           
  
  
                <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link active" href="{{ path('app_main')}}">Accueil</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('app_continent_index')}}">Continents</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('app_pays_index')}}">Pays</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('app_couleur_index')}}">Couleurs</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('app_fabricant_index')}}">Fabricants</a>
                </li>
                 <li class="nav-item">
                    <a class="nav-link" href="#">Marques</a>
                </li>

                <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-bs-toggle="dropdown">
                {% trans %}langues{% endtrans %}
                </a>
                <div class="dropdown-menu">
                <a class="dropdown-item" href="{{path('app_langue_francais')}}" >
                <img src="{{asset('images/francais.jpg')}}" class="rounded" alt="Francais" /> {% trans %}francais{% endtrans %}
                </a>
                <a class="dropdown-item" href="{{path('app_langue_anglais')}}" >
                <img src="{{asset('images/anglais.png')}}" class="rounded" alt="English" /> {% trans %}anglais{% endtrans %}
                </a>
                </div>
                </li>
                </ul>
            </div>
        </nav>

        <header>
         {% block header %}  {% endblock %}
        </header>

        <div class="container">
            {% block body %}{% endblock %}
        </div>


        
        {% block footer %}
            <footer>Carole @AFPA</footer>
        {% endblock %}

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    </body>
</html>
